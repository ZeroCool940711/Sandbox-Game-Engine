					SS_NOTIFY | NOT WS_VISIBLE,166,6,11,10
    CONTROL         "",IDC_UALSTATUS,"Static",SS_LEFTNOWORDWRAP | 
                    SS_PATHELLIPSIS | WS_GROUP | SS_NOTIFY,11,311,175,8
END

